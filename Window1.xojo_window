		Sub Action()
		  Var MyDatePicker As New PickDT
		  
		  If TextField2.Value <> "" Then
		    Try
		      MyDatePicker.currentDate = DateTime.FromString(TextField2.Value)
		    Catch e As RuntimeException
		      MessageBox("Error: " + e.Message)
		    End Try
		  End If
		  
		  // Für Experimente
		  MyDatePicker.AMPM = CheckBox1.Value
		  MyDatePicker.formatMonthDay = CheckBox2.Value
		  MyDatePicker.weekstartsonmonday = CheckBox3.Value
		  MyDatePicker.weekendColored = CheckBox4.Value
		  MyDatePicker.weekendBold = CheckBox5.Value
		  MyDatePicker.pickerTitle = TextField3.Value
		  
		  MyDatePicker.resultTextField = TextField1
		  MyDatePicker.resultAsSQLDateTime = True
		  
		  'MyDatePicker.currentDate = New DateTime(1966,8,27,7,20)
		  
		  MyDatePicker.ShowModal
		  
		End Sub
